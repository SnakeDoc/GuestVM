path ${java.home}/lib/content-types.properties /jdk${java.version}/jre/lib/content-types.properties
path ${java.home}/lib/flavormap.properties /jdk${java.version}/jre/lib/flavormap.properties
path ${java.home}/lib/logging.properties /jdk${java.version}/jre/lib/logging.properties
path ${java.home}/lib/net.properties /jdk${java.version}/jre/lib/net.properties
